    <fields>
        <fullName>Name_for_Clienteling__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL($User.LanguageLocaleKey , &apos;fr&apos;), SAP_Label_FR__c , SAP_Label_EN__c )</formula>
