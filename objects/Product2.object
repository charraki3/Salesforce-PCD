    <fields>
        <fullName>Color_Code_Clienteling__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(   Axis__c , 
&apos;F101&apos;, &apos;#FFE6B4&apos;, 
&apos;F102&apos;, &apos;#FFE6B4&apos;, 
&apos;F110&apos;, &apos;#FFE6B4&apos;, 
&apos;F118&apos;, &apos;#FFE6B4&apos;, 
&apos;F203&apos;,&apos;#ec649d&apos;, 
&apos;F211&apos;, &apos;#FFCCCC&apos;,
&apos;F304&apos;, &apos;#6F79D7&apos;,
&apos;F305&apos;, &apos;#6F79D7&apos;,
&apos;F306&apos;, &apos;#6F79D7&apos;,
&apos;F312&apos;, &apos;#6F79D7&apos;,
NULL
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Color Code Clienteling</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Is_filtered_clienteling__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
							OR(ISPICKVAL(ADV_Status__c, &quot;03&quot;),
										ISPICKVAL(ADV_Status__c, &quot;04&quot;),
										ISPICKVAL(ADV_Status__c, &quot;05&quot;))
 &amp;&amp; 	
			 		OR (Commercial_Use_Text__c = &quot;Catalog Sales&quot;,
							 	 Commercial_Use_Text__c = &quot;Catalog Sales (Instituts)&quot;,
										Commercial_Use_Text__c = &quot;Catalog Sales (TR)&quot;,
								 	Commercial_Use_Text__c = &quot;POS Promotional Items&quot;,
										Commercial_Use_Text__c = &quot;POS Samples&quot;,
										Commercial_Use_Text__c = &quot;Promo Sales Single Item&quot;,
										Commercial_Use_Text__c = &quot;Sales bas. value set&quot;,
										Commercial_Use_Text__c = &quot;Services &amp; Consultations&quot;)
									,
					true,
					false
	)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is filtered clienteling</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
