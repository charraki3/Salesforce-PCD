    <fields>
        <fullName>Case_assessment__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	OR(
		ISPICKVAL(Potential_Serious_Undesirable_Effect__c,&quot;Yes&quot;),
		OR
			(
				INCLUDES(Sensitive_case__c,&quot;Health Professional&quot;),
				INCLUDES(Sensitive_case__c,&quot;Consumer association&quot;),
				INCLUDES(Sensitive_case__c,&quot;Media Exposure&quot;),
				INCLUDES(Sensitive_case__c,&quot;Legal threat&quot;),
				INCLUDES(Sensitive_case__c,&quot;Premium customer&quot;),
				INCLUDES(Sensitive_case__c,&quot;Influencer&quot;)
				
			),
		INCLUDES(Symptoms__c,&quot;9.pimple&quot;),
		INCLUDES(Symptoms__c,&quot;10.pustule&quot;),
		INCLUDES(Symptoms__c,&quot;11.blister&quot;),
		INCLUDES(Symptoms__c,&quot;12.oedema&quot;),
		INCLUDES(Symptoms__c,&quot;13.peeling skin&quot;),
		INCLUDES(Symptoms__c,&quot;14.oozing&quot;),
		INCLUDES(Symptoms__c,&quot;15.erosion&quot;),
		INCLUDES(Symptoms__c,&quot;16.pigmentation&quot;),
		INCLUDES(Symptoms__c,&quot;17.depigmentation&quot;),
		INCLUDES(Symptoms__c,&quot;18.nausea&quot;),
		INCLUDES(Symptoms__c,&quot;19.pain&quot;),
		INCLUDES(Symptoms__c,&quot;20.other&quot;),
		AND(
			OR(INCLUDES(Symptoms__c,&quot;7.watery eyes&quot;),INCLUDES(Symptoms__c,&quot;8.redness&quot;)),
			OR(
				INCLUDES(Symptoms__c,&quot;1.heat sensation&quot;),
				INCLUDES(Symptoms__c,&quot;2.dryness&quot;),
				INCLUDES(Symptoms__c,&quot;3.stinging&quot;),
				INCLUDES(Symptoms__c,&quot;2.dryness&quot;),
				INCLUDES(Symptoms__c,&quot;4.skin tightness&quot;),
				INCLUDES(Symptoms__c,&quot;5.burning sensation&quot;),
				INCLUDES(Symptoms__c,&quot;6.itching&quot;)

			)
		)

	)
,
&quot;INTOLERANCE&quot;
,
IF(
OR(
INCLUDES(Symptoms__c,&quot;1.heat sensation&quot;),
INCLUDES(Symptoms__c,&quot;2.dryness&quot;),
INCLUDES(Symptoms__c,&quot;3.stinging&quot;),
INCLUDES(Symptoms__c,&quot;2.dryness&quot;),
INCLUDES(Symptoms__c,&quot;4.skin tightness&quot;),
INCLUDES(Symptoms__c,&quot;5.burning sensation&quot;),
INCLUDES(Symptoms__c,&quot;6.itching&quot;),
INCLUDES(Symptoms__c,&quot;7.watery eyes&quot;),
INCLUDES(Symptoms__c,&quot;8.redness&quot;)

),


&quot;DISCOMFORT&quot;,
&quot;NOT APPLICABLE&quot;

	)
 )</formula>
